  "scripts": {
    "lint:ts": "eslint domains/**/*.{ts,tsx} --fix",
    "test:cypress": "cypress run --exit-with-code 0",
    "test:jest": "jest --passWithNoTests",
    "prettier": "prettier --write \"domains/**/*.{ts,tsx}\"",
    "dev": "npm run open-browser && next dev",
    "row-dev": "next dev",
    "open-browser": "start http://localhost:3000",
    "build": "next build",
    "start": "next start",
    "test": "jest -c test/jest.config.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false",
    "cypress:test": "start-server-and-test start http://localhost:3000 cypress:run"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint:ts",
      "yarn test:jest"
    ],
    "*.feature": [
      "yarn test:cypress"
    ]
